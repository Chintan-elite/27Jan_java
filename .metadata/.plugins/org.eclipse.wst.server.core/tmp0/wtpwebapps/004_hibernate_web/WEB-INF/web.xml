<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AddProductForm</servlet-name>
  	<display-name>AddProductForm</display-name>
  	<description></description>
  	<servlet-class>controller.AddProductForm</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddNewProduct</servlet-name>
  	<display-name>AddNewProduct</display-name>
  	<description></description>
  	<servlet-class>controller.AddNewProduct</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddProductForm</servlet-name>
  	<url-pattern>/addProduct</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddNewProduct</servlet-name>
  	<url-pattern>/AddNewProduct</url-pattern>
  </servlet-mapping>
</web-app>
